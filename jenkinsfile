pipeline {
    agent any
    stages{
        stage('Clone'){
             steps{  
                 git branch: 'chaima', url: 'https://github.com/chouichi-houda/devopsback.git'
             }
        }  
       stage('MVN CLEAN') {
            steps {
                sh 'mvn clean'
                 
            }
        }
        stage('MVN COMPILE') {
            steps {
                   sh 'mvn compile -e'
                 
            }
        }
          stage('UNIT test'){
            steps{
                sh 'mvn test'
            }
        }
    }
}
